pipeline {
    agent any

    stages {
         stage('SCM') {
            steps {
                git url: 'https://github.com/fabricenankam/simple.git'
            }
        }
        stage('build && SonarQube analysis') {
            steps {
                withSonarQubeEnv(credentialsId: 'Sonarqube-token', installationName:'sonarqube') {
                       sh 'mvn clean package sonar:sonar'
                }
            }
        }
    }
}
